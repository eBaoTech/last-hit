{
	"name": "last-hit",
	"version": "0.3.11",
	"description": "last hit on enterprise web",
	"private": true,
	"main": "main/index.js",
	"scripts": {
		"build:renderer": "npm run build --prefix renderer",
		"start": "npm run build:renderer && electron .",
		"start:dev": "npm run build:renderer && electron . --watch-renderer=true",
		"pack:pre": "export PKG_CONFIG_PATH=/opt/local/lib/pkgconfig:/opt/X11/lib/pkgconfig",
		"pack": "npm run build:renderer && electron-builder --dir",
		"pack:windows": "npm run build:renderer && electron-builder -w",
		"pack:osx": "npm run build:renderer && electron-builder -m",
		"pack:all": "npm run build:renderer && electron-builder -mw",
		"coverage": "nyc report --reporter=html"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/last-hit-aab/last-hit.git"
	},
	"keywords": [
		"test",
		"web",
		"automation"
	],
	"author": "last-hit-team",
	"license": "UNLICENSED",
	"bugs": {
		"url": "https://github.com/last-hit-aab/last-hit/issues"
	},
	"homepage": "https://www.last-hit.com",
	"devDependencies": {
		"electron": "^6.0.11",
		"electron-builder": "^21.2.0",
		"nyc": "^14.1.1"
	},
	"dependencies": {
		"atob": "^2.1.2",
		"body-parser": "^1.19.0",
		"canvas": "^2.6.0",
		"cross-spawn": "^6.0.5",
		"express": "^4.17.1",
		"node-gyp": "^6.0.0",
		"pino": "^5.13.4",
		"pino-pretty": "^3.2.1",
		"puppeteer": "^1.20.0",
		"puppeteer-to-istanbul": "^1.2.2",
		"resemblejs": "^3.2.3",
		"uuid": "^3.3.3",
		"watchr": "^4.1.0",
		"yargs": "^13.3.0"
	},
	"build": {
		"asar": true,
		"asarUnpack": "node_modules/puppeteer/.local-chromium/**/*",
		"artifactName": "${productName}-${version}-${os}-${arch}.${ext}"
	}
}