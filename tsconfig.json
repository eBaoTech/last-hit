{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"removeComments": false,
		"noImplicitAny": false,
		"suppressImplicitAnyIndexErrors": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"outDir": "runtime"
	},
	"exclude": [
		"extensions",
		"node_modules",
		"renderer",
		"replayer",
		"test-one-extension",
		"types",
		"workspace-extension"
	]
}
